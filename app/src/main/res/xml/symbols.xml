<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="49" android:keyLabel="1" android:keyEdgeFlags="left"/>  //: q
        <Key android:codes="50" android:keyLabel="2"/>                              /// w
        <Key android:codes="51" android:keyLabel="3"/>                              //_ e
        <Key android:codes="52" android:keyLabel="4"/>                              //- r
        <Key android:codes="53" android:keyLabel="5"/>                              //( t
        <Key android:codes="54" android:keyLabel="6"/>                              //) y
        <Key android:codes="55" android:keyLabel="7"/>                              //1 u
        <Key android:codes="56" android:keyLabel="8"/>                              //2 i
        <Key android:codes="57" android:keyLabel="9"/>                              //3 o
        <Key android:codes="48" android:keyLabel="0" android:keyEdgeFlags="right"/> //0 p
    </Row>
    
    <Row>
        <Key android:codes="64" android:keyLabel="\@" android:keyEdgeFlags="left"/> //@ a
        <Key android:codes="35" android:keyLabel="\#"/>                             //' s
        <Key android:codes="36" android:keyLabel="$"/>                              //" d
        <Key android:codes="37" android:keyLabel="%"/>                              //+ f
        <Key android:codes="38" android:keyLabel="&amp;"/>                          //* g
        <Key android:codes="42" android:keyLabel="*"/>                              //# h
        <Key android:codes="45" android:keyLabel="-"/>                              //4 j
        <Key android:codes="61" android:keyLabel="="/>                              //5 k
        <Key android:codes="40" android:keyLabel="("/>                              //6 l
        <Key android:codes="41" android:keyLabel=")" android:keyEdgeFlags="right"/> //EMPTY bckspc
    </Row>
    
    <Row>
        <Key android:codes="33" android:keyLabel="!" android:keyEdgeFlags="left"/>  //! z
        <Key android:codes="34" android:keyLabel="&quot;"/>                         //? x
        <Key android:codes="39" android:keyLabel="\'"/>                             //, c
        <Key android:codes="58" android:keyLabel=":"/>                              //. v
        <Key android:codes="63" android:keyLabel="\?" android:keyWidth="20%p" />    //EMPTY space
        <Key android:codes="59" android:keyLabel=";"/>                              //7 b
        <Key android:codes="47" android:keyLabel="/" />                             //8 n
        <Key android:codes="63" android:keyLabel="\?"/>                             //9 m
        <Key android:codes="63" android:keyLabel="\?" android:keyEdgeFlags="right"/>//EMPTY enter
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="1" android:keyLabel="L" android:keyWidth="25%p" android:keyEdgeFlags="left" android:isRepeatable="true"/>
        <Key android:codes="9" android:keyLabel="TAB" android:keyWidth="15%p"/>
        <Key android:codes="-2" android:keyLabel="SYM" android:keyWidth="20%p"/>
        <Key android:codes="9" android:keyLabel="CTRL" android:keyWidth="15%p"/>
        <Key android:codes="2" android:keyLabel="R" android:keyWidth="25%p" android:keyEdgeFlags="right" android:isRepeatable="true"/>
    </Row>

